{
  "name": "Roact Test Place",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",

      "Roact": {
        "$path": "src"
	  },

      "RoactSnapshots": {
        "$path": "RoactSnapshots"
      },

      "TestEZ": {
        "$path": "modules/testez/lib"
      }
    },

    "StarterPlayer": {
      "$className": "StarterPlayer",

      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",

        "RoactExamples": {
          "$path": "examples"
        }
      }
    },

    "ServerScriptService": {
      "$className": "ServerScriptService",

      "RoactBenchmark": {
        "$path": "benchmarks"
      },
      "RoactTests": {
        "$path": "bin/run-tests.server.lua"
      }
    },

    "HttpService": {
      "$className": "HttpService",
      "$properties": {
        "HttpEnabled": true
      }
    },

    "Players": {
      "$className": "Players",
      "$properties": {
        "CharacterAutoLoads": false
      }
    }
  }
}